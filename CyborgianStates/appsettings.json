{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Verbose"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "Logs\\CyborgianStates_.log",
                "rollingInterval": "Day",
                "outputTemplate": "{Timestamp:dd.MM.yy HH:mm:ss zzz} {Level:w4}: {SourceContext} {Properties} {Message:l} {Exception} {NewLine}"
              }
            },
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "{Level:w4}: {SourceContext} {Properties} {Message:l} {Exception} {NewLine}",
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleThemes::Code, Serilog.Sinks.Console"
              }
            }
          ]
        }
      }
    ]
  },
  "Configuration": {
    "DiscordBotLoginToken": "",
    "ExternalAdminUserId": "0",
    "SeperatorChar": "",
    "DbConnection": "",
    "Contact": "",
    "Locale": "en-US",
    "InputChannel": "Console"
  }
}